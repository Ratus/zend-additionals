{
    "name": "ratus/zend-additionals",
    "description": "Ratus Zend (ZF2) Additionals",
    "type": "library",
    "keywords": [
        "framework",
        "ratus",
        "zend",
        "additionals",
        "zf2"
    ],
    "homepage": "http://www.ratus.nl/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-cache": "2.*",
        "zendframework/zend-db": "2.*",
        "zendframework/zend-stdlib": "2.*"
    },
    "autoload": {
        "psr-0": {
            "ZendAdditionals\\": "library/ZendAdditionals"
        }
    },
    "replace": {
        "ratus/ratus-cache": "self.version",
        "ratus/ratus-db": "self.version",
        "ratus/ratus-stdlib": "self.version",
        "ratus/ratus-ssh": "self.version"
    }
}

